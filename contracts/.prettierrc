{
	"plugins": ["prettier-plugin-solidity"],
	"trailingComma": "es5",
	"tabWidth": 4,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"overrides": [
		{
			"files": "*.sol",
			"options": {
				"parser": "solidity-parse",
				"compiler": "0.8.24",
				"printWidth": 80,
				"tabWidth": 4,
				"useTabs": false,
				"singleQuote": false,
				"bracketSpacing": false
			}
		}
	]
}